{
  "name": "@cell-diagrams/collab-client",
  "version": "0.1.0",
  "description": "Yjs collaboration client for Cell Diagrams",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "y-protocols": "^1.0.6",
    "y-websocket": "^2.0.4",
    "yjs": "^13.6.20"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "tsup": "^8.2.0",
    "typescript": "^5.5.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "keywords": ["cell-diagrams", "yjs", "collaboration", "crdt"],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/cell-diagrams",
    "directory": "packages/collab-client"
  }
